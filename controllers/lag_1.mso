using "types";
 
 
Model Lag_1

ATTRIBUTES
	Pallete 	= false;
	Icon 		= "icon/Lag_1"; 
	Brief 		= "Model Lag.";
	Info 		=
"== Inputs ==
* One input signal.
	
== Outputs ==
* One output signal.
";
	
	PARAMETERS
	gain as positive (Brief="model gain");
	tau  as positive (Brief="model time constant", Unit='s');
	
	VARIABLES
	input  as Real  (Brief="input signal");
	output as Real  (Brief="output signal");
	
	EQUATIONS
	
	"Calculate output"
	gain*input= tau*diff(output) + output;
	
	INITIAL
	diff(output)=0/'s';
	
end
